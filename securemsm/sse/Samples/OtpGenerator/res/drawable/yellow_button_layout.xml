<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <solid android:color="#FACC2E" />

            <stroke android:width="1dp" android:color="#FFBF00" />

            <corners android:radius="3dp" />

            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape></item>
    <item android:state_enabled="false"><shape>
            <gradient android:angle="270" android:endColor="#7D7878" android:startColor="#807878" />

            <stroke android:width="1dp" android:color="#7D7878" />

            <corners android:radius="4dp" />

            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape></item>
    <item><shape>
            <gradient android:angle="270" android:endColor="#FFBF00" android:startColor="#FACC2E" />

            <stroke android:width="1dp" android:color="#FFBF00" />

            <corners android:radius="4dp" />

            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape></item>

</selector>
